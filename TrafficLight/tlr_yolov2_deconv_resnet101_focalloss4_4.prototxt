layer {
	name: "neg_data"
	type: "ImageData"
	top: "neg_data"
	top: "dummy_label"
	include: { phase: TRAIN}
	image_data_param {
		source: "C:/Users/jylee/Desktop/TLR/tl-background/list.txt"
		root_folder: "C:/Users/jylee/Desktop/TLR/tl-background"
		batch_size: 2
		shuffle: true
		new_height: 224
		new_width: 224
	}

	#transform_param {
	#	mirror: true
	#}
}

layer {
	name: "dummy_label_ignore"
	type: "Silence"
	bottom: "dummy_label"
	include: { phase: TRAIN}
}

layer {
	name: "neg_gt"
	type: "NegGT"
	bottom: "label_bosch_pos"
	bottom: "bbox_bosch"
	top: "neg_label"
	top: "neg_bbox"
	include: {phase: TRAIN}
	neg_gt_param {
		batch_size: 2
		num_gt: 15
	}
}

layer {
	name: "data_concat"
	type: "Concat"
	bottom: "data_bosch"
	bottom: "neg_data"
	top: "data"

	include: { phase: TRAIN}

	concat_param {
		axis: 0
	}
}

layer {
	name: "label_concat"
	type: "Concat"
	bottom: "label_bosch_pos"
	bottom: "neg_label"
	top: "label_bosch"

	include: { phase: TRAIN}

	concat_param {
		axis: 0
	}
}

layer {
	name: "bbox_concat"
	type: "Concat"
	bottom: "bbox_bosch"
	bottom: "neg_bbox"
	top: "bbox"

	include: { phase: TRAIN}

	concat_param {
		axis: 0
	}
}



layer {
  name: 'data'
  type: 'MinibatchData'
  top: 'data'
  top: 'label_bosch'
  top: 'bbox'

  include: { phase: TEST}

  data_param {
    #source: "D:/DB/traffic_light/bosch/lmdb/bosch_test_shuffle/"
    source: "D:/DB/traffic_light/lisa-traffic-light-dataset/lmdb/test_shuffle/"
    batch_size: 1
    backend: LMDB
  }

  minibatch_data_param {
    num_batch: 3
    width: 224
    height: 224
    num_gt: 15
    max_num_patch: 10
  }
}

