layer {
  name: 'data'
  type: 'MinibatchData'
  top: 'data_bosch'
  top: 'label_bosch_pos'
  top: 'bbox_bosch'#
  include: { phase: TRAIN}#
  data_param {
    #source: "D:/DB/traffic_light/bosch/lmdb/bosch_train_add_shuffle/"
    #source: "D:/DB/traffic_light/bosch/lmdb/bosch_train_add_allsize_shuffle_augmented/"
    #source: "D:/DB/traffic_light/bosch/lmdb/bosch_test_shuffle/"
    #source: "D:/DB/traffic_light/bosch/multi_scale/pos_only_lmdb/"
    source: "C:/Users/jylee/Desktop/TLR/pos_only_lmdb(train)"
    batch_size: 1
    backend: LMDB
  }#
  augmentation_param{
    rand_resize_param {
      p: 0.8
      w_min: 0.9
      w_max: 1.1
      h_min: 0.9
      h_max: 1.1
    }#
    gauss_noise_param {
      p: 0.3
      mean: 0
      stddev: 0.01
    }#
    mirror_param {
      p: 0.5
    }#
    blur_param {
      p: 0.1
      kernel_size: 3
      sigma: 0.2
    }
  }#
  minibatch_data_param {
    num_batch: 3
    width: 224
    height: 224
    num_gt: 15
    max_num_patch: 4
  }
}

